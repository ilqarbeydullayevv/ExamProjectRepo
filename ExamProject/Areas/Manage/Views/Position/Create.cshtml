@using ExamProject.ViewModel.Position
@model CreatePositionvm

<div class="Countainer">
    <form method="post">
        <div class="form-group">
            <label asp-for="Name">Name</label>
            <input class="form-control" asp-for="Name" placeholder="Enter Name">
